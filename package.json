{
  "main": "2024-ICSC.html",
  "name": "2024-ICSC",
  "description": "HTML5 with Csound",
  "version": "0.1.0",
  "keywords": [ "Csound", "node-webkit" ],
  "nodejs": true,
  "window": {
    "title": "2024-ICSC",
    "icon": "link.png",
    "toolbar": true,
    "frame": true,
    "maximized": true,
    "position": "mouse",
    "fullscreen": true
  },
  "webkit": {
    "plugin": true
  },
  "chromium-args": {
    "--enable-logging": true,
    "--context-mixed": true,
    "--v=1": 1,
    "--device-scale-factor": 4
  }
}